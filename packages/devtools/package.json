{
	"name": "@tanstack/devtools",
	"version": "1.1.2",
	"description": "Minimal open-source stack to help you ship an open-source package in TS",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		"./input.css": "./dist/index.css",
		".": {
			"import": {
				"types": "./dist/esm/index.d.ts",
				"default": "./dist/esm/index.js"
			},
			"require": {
				"types": "./dist/cjs/index.d.cts",
				"default": "./dist/cjs/index.cjs"
			}
		}
	},
	"scripts": {
		"test:cov": "vitest run --coverage",
		"build": "vite build",
		"dev": "vite dev",
		"prepublishOnly": "pnpm run build",
		"typecheck": "tsc",
		"validate": "pnpm run check && pnpm run typecheck && pnpm run test",
		"check:exports": "attw --pack ."
	},
	"author": "",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/forge-42/@tanstack/devtools.git"
	},
	"bugs": {
		"url": "https://github.com/forge-42/@tanstack/devtools/issues"
	},
	"files": ["dist"],
	"homepage": "https://github.com/forge-42/@tanstack/devtools#readme",
	"publishConfig": {
		"provenance": true
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.17.4",
		"@changesets/cli": "^2.29.0",
		"@tanstack/config": "^0.20.0",
		"@types/node": "^20.17.30",
		"@vitest/coverage-v8": "^3.1.1",
		"autoprefixer": "^10.4.21",
		"happy-dom": "^17.4.4",
		"rollup-plugin-postcss": "^4.0.2",
		"solid-js": "^1.8.0",
		"tailwindcss": "^3.4.17",
		"tailwindcss-animate": "^1.0.7",
		"tsdown": "^0.9.1",
		"typescript": "^5.8.3",
		"vite": "^6.2.6",
		"vite-plugin-solid": "^2.11.7",
		"vitest": "^3.1.1"
	},
	"dependencies": {
		"clsx": "^2.1.1",
		"solid-js": "^1.9.7",
		"tailwind-merge": "^3.3.1"
	}
}
